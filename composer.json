{
  "name": "hananprogram/laravel-l10n",
  "description": "DB-backed i18n with auto-fill and admin UI",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
    "spatie/laravel-translation-loader": "^2.8",
    "stichoza/google-translate-php": "^5.3"
  },
  "autoload": {
  "psr-4": {
    "HananProgram\\L10n\\": "src/"
  },
  "files": ["src/helpers.php"]
},
  "extra": {
    "laravel": {
      "providers": [
        "HananProgram\\L10n\\L10nServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
